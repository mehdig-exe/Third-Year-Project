   "source": [
    "x1 = np.genfromtxt('AngleData.csv',delimiter=',')"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": null,
   "metadata": {},
   "outputs": [],
   "source": [
    "angleArray,index = [],[]\n",
    "\n",
    "for i,j in enumerate(x1[0:len(x1)-1]):\n",
    "    a = x1[i]\n",
    "    b = x1[i+1]\n",
    "    A = np.linalg.norm(a)\n",
    "    B = np.linalg.norm(b)\n",
    "    ans = np.arccos((np.dot(a,b))/(A*B))\n",
    "    angleArray.append(ans)\n",
    "    index.append(i)"
   ]
  },
